  "keywords": [
    "weixin","api"
  ],
  "homepage": "http://github.com/lanizz/weixin",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Jinming Lan",
      "email": "lanjinming@hotmail.com",
      "homepage": "http://blog.binblue.net"
    }
