  "require": {
    "php": ">=5.5.9",
    "lanizz/php-helper":"dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "autoload": {
    "psr-4": {
      "Lanizz\\Weixin\\": "src/",
      "Lanizz\\Weixin\\Request\\": "src/Request/",
      "Lanizz\\Weixin\\Http\\": "src/Http/",
